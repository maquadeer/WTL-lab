<html>
  <head>
    <title>Marks Memo</title>
  </head>
  <body>
    <center><h1>Marks Memo</h1>
        <table width="60%" border="1">
            <tr>
                <th>S.No</th>
                <th>Code</th>
                <th>Subject</th>
                <th>Internal Marks</th>
                <th>External Marks</th>
            </tr>
        <% data.forEach(function(data) { %>
            <tr align="center">
                <td><%= data.id %></td>
                <td><%= data.code %></td>
                <td align="left"><%= data.sub %></td>
                <td><%= data.imarks %></td>
                <td><%= data.emarks %></td>
            </tr>
      <% }) %>
    </table>
    </center>
  </body>
</html>

